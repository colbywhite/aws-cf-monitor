{
  "name": "aws-cf-monitor",
  "version": "1.0.4",
  "description": "A wrapper around the AWS CloudFormation Node API that monitors the progress of the CF commands while providing smart logging",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc --pretty",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jasmine-ts --config=./jasmine.json",
    "test:int": "ts-node test/int-test.ts",
    "test:watch": "nodemon --ext ts --exec 'jasmine-ts --config=./jasmine.json'"
  },
  "keywords": [
    "AWS",
    "CloudFormation"
  ],
  "author": "Colby M. White",
  "license": "ISC",
  "repository": "colbywhite/aws-cf-monitor",
  "dependencies": {
    "aws-sdk": "2.464.0",
    "chalk": "2.4.2",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/faker": "4.1.5",
    "@types/jasmine": "3.3.13",
    "@types/node": "12.0.3",
    "@types/sinon": "7.0.12",
    "@types/winston": "2.4.4",
    "aws-sdk-mock": "4.4.0",
    "faker": "4.1.0",
    "jasmine": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "jasmine-ts": "0.3.0",
    "nodemon": "1.19.1",
    "sinon": "7.3.2",
    "ts-node": "8.2.0",
    "tslint": "5.16.0",
    "typescript": "3.4.5"
  }
}
